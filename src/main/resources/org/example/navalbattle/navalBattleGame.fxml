<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.Region?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
            prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="org.example.navalbattle.controller.GameNavalBattleController" fx:id="gamePane">
   <HBox spacing="60" AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0" AnchorPane.topAnchor="0" AnchorPane.bottomAnchor="0" alignment="CENTER">
      <Region fx:id="leftSpacer" HBox.hgrow="ALWAYS"/>
      <GridPane fx:id="gridPane">
         <padding>
            <Insets top="10" right="5" bottom="10" left="10"/> <!-- Ajuste de relleno -->
         </padding>
      </GridPane>
      <GridPane fx:id="shipGridPane">
         <padding>
            <Insets top="10" right="10" bottom="10" left="5"/> <!-- Ajuste de relleno -->
         </padding>
      </GridPane>
      <Region fx:id="rightSpacer" HBox.hgrow="ALWAYS"/>
   </HBox>
</AnchorPane>
